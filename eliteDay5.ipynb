      }
    },
    {
      "cell_type": "code",
      "source": [
        "#leetcode 102\n",
        "class Solution(object):\n",
        "    def levelOrder(self, root):\n",
        "        \"\"\"\n",
        "        :type root: TreeNode\n",
        "        :rtype: List[List[int]]\n",
        "        \"\"\"\n",
        "        if root is None:\n",
        "            return []\n",
        "        res = []\n",
        "        q = []\n",
        "        q.append(root)\n",
        "        while q:\n",
        "            l = len(q)\n",
        "            next_level, val = [], []\n",
        "            for i in range(l):\n",
        "                val.append(q[i].val)\n",
        "                if q[i].left:\n",
        "                    next_level.append(q[i].left)\n",
        "                if q[i].right:\n",
        "                    next_level.append(q[i].right)  \n",
        "            q = next_level\n",
        "            res.append(val)\n",
        "        return res"
      ],
