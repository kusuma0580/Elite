        "6)zigzag level order traversal"
      ],
      "metadata": {
        "id": "AXChWSMUXMcq"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "#leetcode 103\n",
        "class Solution(object):\n",
        "    def zigzagLevelOrder(self, root):\n",
        "        \"\"\"\n",
        "        :type root: TreeNode\n",
        "        :rtype: List[List[int]]\n",
        "        \"\"\"\n",
        "        res = []\n",
        "        q = []\n",
        "        if root:\n",
        "            q.append(root)\n",
        "        while q:\n",
        "            res.append([node.val for node in q])\n",
        "            next_level = []\n",
        "            for node in q:\n",
        "                if node.left:\n",
        "                    next_level.append(node.left)\n",
        "                if node.right:\n",
        "                    next_level.append(node.right)\n",
        "            q = next_level\n",
        "        for i in range(1, len(res),2):\n",
        "            res[i].reverse()\n",
        "        return res"
      ],
