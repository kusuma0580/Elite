    },
    {
      "cell_type": "code",
      "source": [
        "#leetcode 297\n",
        class Codec:

    def serialize(self, root):
        """Encodes a tree to a single string.
        
        :type root: TreeNode
        :rtype: str
        """
        if root is None:
            return ''
        res = ''
        q = []
        q.append(root)
        while q:
            node = q.pop(0)
            if node:
                res += str(node.val) + ' '
                q.append(node.left)
                q.append(node.right)
            else:
                res += 'n '
        return res
        

    def deserialize(self, data):
        """Decodes your encoded data to tree.
        
        :type data: str
        :rtype: TreeNode
        """
        if not data:
            return None
        val = data.split()
        root = TreeNode(val[0])
        q = [root]
        for i in range(1, len(val), 2):
            node = q.pop(0)
            if val[i] != 'n':
                node.left = TreeNode(val[i])
                q.append(node.left)
            if val[i + 1] != 'n':
                node.right = TreeNode(val[i + 1])
                q.append(node.right)
        return root

